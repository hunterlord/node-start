    "license": "ISC",
    "main": "app/index.js",
    "name": "node-start",
    "scripts": {
        "build": "rm -rf ./dist && npm run build-assets && npm run build-server",
        "build-assets": "cross-env NODE_ENV=production node ./node_modules/webpack/bin/webpack.js",
        "build-server": "babel src -d dist --copy-files",
        "prepare": "npm i -g babel-cli babel pm2 cross-env browser-sync",
        "start": "npm run start-front && npm run start-server",
        "start-front": "pm2 start process.config.js --only front-dev",
        "start-prod": "npm run build && pm2 start process.config.js --only server",
        "start-server": "pm2 start process.config.js --only server-dev",
        "start-sync": "browser-sync start --config bs-config.js"
    },
    "version": "1.0.0"
